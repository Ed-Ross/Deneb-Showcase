      "transform": [
        {"type": "extent", "field": "No", "signal": "myExtent"},
        {
          "type": "formula",
          "as": "No",
          "expr": "datum.Prospective?datum.No+'​':datum.No"
        },
        {
          "type": "formula",
          "as": "Alive Label",
          "expr": "datum.Alive?'Alive':'Dead'"
        },
